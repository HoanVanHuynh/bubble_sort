# Python Program for Bubble Sort
arr2 = [7, 5, 9, 3, 6, 2, 1]

arr = [3, 5, 2, 4, 1]
n = len(arr)

for i in range(n): # need to check this range(n) again
	for j in range(0, n-i-1):
		print("n = ", n)
		print("n-i-1 = ", n-i-1)
		print('arr[j] = ', arr[j])
		print('arr[j+1] = ', arr[j+1])
		if arr[j] > arr[j+1]:
			temp = arr[j+1]
			arr[j+1] = arr[j]
			arr[j] = temp
print("Sorted Array: ", arr)
